{
  "name": "brain-reducer",
  "displayName": "Brain Reducer",
  "description": "AI-powered coding assistant with Claude and Gemini integration",
  "version": "0.1.3",
  "publisher": "longlt201203",
  "engines": {
    "vscode": "^1.98.0"
  },
  "categories": [
    "Other",
    "Programming Languages",
    "Machine Learning",
    "Snippets"
  ],
  "keywords": [
    "ai",
    "claude",
    "gemini",
    "coding assistant",
    "code generation"
  ],
  "icon": "resources/icon.png",
  "galleryBanner": {
    "color": "#1e1e1e",
    "theme": "dark"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/longlt201203-official/brain-reducer"
  },
  "homepage": "https://github.com/longlt201203-official/brain-reducer",
  "bugs": {
    "url": "https://github.com/longlt201203-official/brain-reducer/issues"
  },
  "license": "MIT",
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "brainReducer.openChat",
        "title": "Brain Reducer: Open Chat"
      },
      {
        "command": "brainReducer.setApiKey",
        "title": "Brain Reducer: Set API Key"
      },
      {
        "command": "brainReducer.setChromeLocation",
        "title": "Brain Reducer: Set Chrome Location"
      }
    ],
    "configuration": {
      "title": "Brain Reducer Settings",
      "properties": {
        "brainReducer.apiKey": {
          "type": "string",
          "default": "",
          "description": "API key for the AI service."
        },
        "brainReducer.model": {
          "type": "string",
          "default": "gemini-2.0-flash",
          "description": "Current AI Model"
        },
        "brainReducer.chromeLocation": {
          "type": "string",
          "default": "",
          "description": "Path to the Chrome executable for web scraping."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "vscode:package": "vsce package --yarn",
    "vscode:publish": "vsce publish --yarn"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/parse-gitignore": "^1.0.2",
    "@types/vscode": "^1.98.0",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "@vscode/vsce": "^3.3.0",
    "eslint": "^9.21.0",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "@google/generative-ai": "^0.24.0",
    "@langchain/anthropic": "^0.3.24",
    "@langchain/core": "^0.3.61",
    "@langchain/google-genai": "^0.2.14",
    "axios": "^1.6.0",
    "ejs": "^3.1.10",
    "gpt-tokenizer": "^2.9.0",
    "ignore": "^7.0.3",
    "langchain": "^0.3.29",
    "parse-gitignore": "^2.0.0",
    "puppeteer-core": "^24.12.0",
    "zod": "^3.25.71"
  }
}